{
  "info": {
    "name": "Oral Lesion Classifier API",
    "description": "API for detecting and classifying oral lesions using deep learning",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json",
    "_exporter_id": "oral-lesion-classifier"
  },
  "item": [
    {
      "name": "Health Check",
      "item": [
        {
          "name": "Root Health Check",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/",
              "host": ["{{base_url}}"],
              "path": [""]
            }
          },
          "response": []
        },
        {
          "name": "Detailed Health Check",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/health",
              "host": ["{{base_url}}"],
              "path": ["health"]
            }
          },
          "response": []
        }
      ]
    },
    {
      "name": "Classification",
      "item": [
        {
          "name": "Get Available Classes",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/classes",
              "host": ["{{base_url}}"],
              "path": ["classes"]
            }
          },
          "response": []
        },
        {
          "name": "Predict Single Image",
          "request": {
            "method": "POST",
            "header": [],
            "body": {
              "mode": "formdata",
              "formdata": [
                {
                  "key": "file",
                  "type": "file",
                  "src": "/path/to/your/image.jpg"
                }
              ]
            },
            "url": {
              "raw": "{{base_url}}/predict",
              "host": ["{{base_url}}"],
              "path": ["predict"]
            }
          },
          "response": []
        },
        {
          "name": "Batch Predict Multiple Images",
          "request": {
            "method": "POST",
            "header": [],
            "body": {
              "mode": "formdata",
              "formdata": [
                {
                  "key": "files",
                  "type": "file",
                  "src": "/path/to/image1.jpg"
                },
                {
                  "key": "files",
                  "type": "file",
                  "src": "/path/to/image2.jpg"
                }
              ]
            },
            "url": {
              "raw": "{{base_url}}/batch-predict",
              "host": ["{{base_url}}"],
              "path": ["batch-predict"]
            }
          },
          "response": []
        }
      ]
    }
  ],
  "variable": [
    {
      "key": "base_url",
      "value": "http://localhost:8000",
      "type": "string"
    }
  ]
}

